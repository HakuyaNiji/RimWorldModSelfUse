	<!--=================================== chemfuel ==========================================-->
	<RecipeDef>
		<defName>Make5ChemfuelFromWood</defName>
		<label>make chemfuel from wood x5</label>
		<description>Make a batch of chemfuel by gasifying wood logs.</description>
		<jobString>Refining chemfuel from wood.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>10000</workAmount>
		<workSkill>Intellectual</workSkill>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>WoodLog</li>
					</thingDefs>
				</filter>
				<count>350</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<thingDefs>
				<li>WoodLog</li>
			</thingDefs>
		</fixedIngredientFilter>
		<products>
			<Chemfuel>175</Chemfuel>
		</products>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
	</RecipeDef>
	<RecipeDef>
		<defName>Make5ChemfuelFromOrganics</defName>
		<label>make chemfuel from organic x5</label>
		<description>Make a batch of chemfuel by extracting and condensing wood gas from logs.</description>
		<jobString>Refining chemfuel from organics.</jobString>
		<effectWorking>Cremate</effectWorking>
		<soundWorking>Recipe_Cremate</soundWorking>
		<workAmount>12500</workAmount>
		<workSkill>Intellectual</workSkill>
		<allowMixingIngredients>true</allowMixingIngredients>
		<ingredientValueGetterClass>IngredientValueGetter_Nutrition</ingredientValueGetterClass>
		<ingredients>
			<li>
				<filter>
					<categories>
						<li>Foods</li>
						<li>PlantMatter</li>
					</categories>
					<disallowedThingDefs>
						<li>Hay</li>
					</disallowedThingDefs>
				</filter>
				<count>17.5</count>
			</li>
		</ingredients>
		<fixedIngredientFilter>
			<categories>
				<li>Foods</li>
				<li>PlantMatter</li>
			</categories>
			<disallowedThingDefs>
				<li>Hay</li>
			</disallowedThingDefs>
		</fixedIngredientFilter>
		<defaultIngredientFilter>
			<categories>
				<li>FoodRaw</li>
			</categories>
			<disallowedCategories>
				<li>MeatRaw</li>
				<li>AnimalProductRaw</li>
			</disallowedCategories>
		</defaultIngredientFilter>
		<products>
			<Chemfuel>175</Chemfuel>
		</products>
		<recipeUsers>
			<li>BiofuelRefinery</li>
		</recipeUsers>
	</RecipeDef>
