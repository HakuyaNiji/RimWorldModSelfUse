	<!--=================================== smelt metal ==========================================-->
	<RecipeDef>
		<defName>Extract5MetalFromSlag</defName>
		<label>smelt metal from slag x5</label>
		<description>Use heat and strong electromagnets to extract useful metal from slag chunks.</description>
		<jobString>Smelting metal from slag.</jobString>
		<workAmount>1000</workAmount>
		<workSpeedStat>SmeltingSpeed</workSpeedStat>
		<effectWorking>Smelt</effectWorking>
		<soundWorking>Recipe_Smelt</soundWorking>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>ChunkSlagSteel</li>
					</thingDefs>
				</filter>
				<count>5</count>
			</li>
		</ingredients>
		<!-- Switched to standard products so we can do "do until you have X"
    <specialProducts>
      <li>Smelted</li>
    </specialProducts>-->
		<products>
			<Steel>75</Steel>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>ChunkSlagSteel</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>ElectricSmelter</li>
		</recipeUsers>
	</RecipeDef>
