<Defs>
	<!--=================================== Medicine ==========================================-->
	<RecipeDef>
		<defName>Make20Medicine</defName>
		<label>Make medicine x20</label>
		<description>Make Medicines.</description>
		<jobString>Making Medicines.</jobString>
		<workSkill>Intellectual</workSkill>
		<effectWorking>Cook</effectWorking>
		<soundWorking>Recipe_CookMeal</soundWorking>
		<workSpeedStat>DrugSynthesisSpeed</workSpeedStat>
		<workAmount>10200</workAmount>
		<ingredients>
			<li>
				<filter>
					<thingDefs>
						<li>MedicineHerbal</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Neutroamine</li>
					</thingDefs>
				</filter>
				<count>20</count>
			</li>
			<li>
				<filter>
					<thingDefs>
						<li>Cloth</li>
					</thingDefs>
				</filter>
				<count>60</count>
			</li>
		</ingredients>
		<products>
			<MedicineIndustrial>20</MedicineIndustrial>
		</products>
		<fixedIngredientFilter>
			<thingDefs>
				<li>MedicineHerbal</li>
				<li>Neutroamine</li>
				<li>Cloth</li>
			</thingDefs>
		</fixedIngredientFilter>
		<recipeUsers>
			<li>DrugLab</li>
		</recipeUsers>
		<skillRequirements>
			<Intellectual>4</Intellectual>
			<Crafting>10</Crafting>
		</skillRequirements>
		<researchPrerequisite>MedicineProduction</researchPrerequisite>
	</RecipeDef>
